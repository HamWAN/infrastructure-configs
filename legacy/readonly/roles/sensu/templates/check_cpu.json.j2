{
  "checks": {
    "cpu_metrics": {
      "type": "metric",
      "command": "metrics-cpu.rb",
      "subscribers": [
        "production"
      ],
      "interval": 10
    },
    "cpu_check": {
      "command": "check-cpu.rb",
      "subscribers": [
        "production"
      ],
      "interval": 10,
      "occurrences": 12,
      "refresh": 60
    }
  }
}
